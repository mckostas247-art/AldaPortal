# Example override file for local development
# Copy this to docker-compose.override.yml and customize as needed
# docker-compose.override.yml is automatically loaded by docker-compose

version: '3.8'

services:
  mysql:
    ports:
      - "3306:3306"
    environment:
      MYSQL_ROOT_PASSWORD: root
      MYSQL_DATABASE: PortalBase
      MYSQL_USER: portalbase
      MYSQL_PASSWORD: root

  web:
    ports:
      - "8080:8080"
    environment:
      - ASPNETCORE_ENVIRONMENT=Development
      - ASPNETCORE_URLS=http://+:8080



